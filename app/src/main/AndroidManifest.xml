<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schedulegenerator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ScheduleGenerator">
        <activity android:name=".Controllers.ProcessProjectActivity"></activity>
        <activity android:name=".Controllers.CollabActivity" />
        <activity android:name=".Controllers.UserProfileActivity" />
        <activity android:name=".Controllers.RequestProfileActivity" />
        <activity android:name=".Controllers.EditProjectActivity" />
        <activity android:name=".Controllers.RequestProjectActivity" />
        <activity android:name=".Controllers.ProjectProfileActivity" />
        <activity android:name=".Controllers.SignUpActivity" />
        <activity android:name=".Controllers.AddProjectActivity" />
        <activity android:name=".Controllers.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controllers.MainActivity" />
    </application>

</manifest>